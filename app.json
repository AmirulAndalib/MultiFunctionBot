{
  "name": "MultiFunction_Bot",
  "description": "Telegram Bot to Bypass Short Links and get direct URLs",
  "keywords": [
      "telegram",
      "bypass",
      "adlinks",
      "python",
      "bot"
    ],
  "repository": "https://github.com/WizardX22/MultiFunction_Bot/",
  "success_url": "/",
  "env": {
      "API_ID": {
          "description": "Get this value from https://my.telegram.org",
		  "value":"",
